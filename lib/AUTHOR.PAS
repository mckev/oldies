UNIT AUTHOR;

INTERFACE

VAR  MAKER : STRING [16];

IMPLEMENTATION

PROCEDURE SET_AUTHOR;
CONST SSSS : STRING[16] = #164#43#30#220#217#154#111#203#133#35#54#188#73#82#102#198;
VAR PPPP,QQQQ : POINTER;
BEGIN
   PPPP := ADDR (SSSS);
   QQQQ := ADDR (MAKER);
   ASM
      PUSH DS
      MOV CX,16
      LES DI,QQQQ
      LDS SI,PPPP
      MOVSB
      @1:
      LODSB
      NOT AL
      ROL AL,CL
      XOR AL,CL
      STOSB
      LOOP @1
      POP DS
   END;
END;

BEGIN
   SET_AUTHOR;
END.